{
  "name": "sass-bulma-dashboard-exact",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "npm-run-all -p watch:css serve",
    "serve": "live-server --port=5173 --open=./index.html",
    "watch:css": "sass --load-path=node_modules --watch scss/main.scss:css/main.css",
    "build:css": "sass --load-path=node_modules scss/main.scss css/main.css --style=compressed --no-source-map"
  },
  "devDependencies": {
    "bulma": "^1.0.2",
    "live-server": "^1.2.2",
    "npm-run-all": "^4.1.5",
    "sass": "^1.77.0"
  }
}